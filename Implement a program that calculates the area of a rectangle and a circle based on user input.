#include <stdio.h>

int main()
{
    int l,b,r;
    printf("enter the length :");
scanf("%d",&l);
printf("enter the breadth :");
scanf("%d",&b);
int areaofrectangle=l*b;
printf("area of rectangle is:%d\n",areaofrectangle);
printf("enter the radius of circle:");
scanf("%d",&r);
float areaofcircle=3.14*r*r;
printf("area of circle is:%f",areaofcircle);

    return 0;
